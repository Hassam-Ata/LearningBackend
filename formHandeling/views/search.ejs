<!DOCTYPE html>
<html>

<head>
    <title>
        <%= title %>
    </title>
</head>

<body>
    <h1>
        <%= title %>
    </h1>
    <% if (users.length> 0) { %>
        <ul>
            <% users.forEach(user=> { %>
                <li>
                    Name: <%= user.firstName %>
                        <%= user.lastName %> <br>
                            Email: <%= user.email %> <br>
                                Age: <%= user.age %> <br>
                                    Bio: <%= user.bio %>
                </li>
                <% }) %>
        </ul>
        <% } else { %>
            <p>No users found.</p>
            <% } %>
                <a href="/">Back to home</a>
</body>

</html>